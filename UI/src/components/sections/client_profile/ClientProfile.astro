---
import type { ClientsContent } from "@tsTypes/index";
import { Text } from "@components/shared";
import { ClientProfileCard } from "@components/blocks";

interface Props {
  works: ClientsContent[];
}

const { works } = Astro.props;
---

<section class="flex flex-col gap-20 mt-20">

  {
    works?.map(({ data, slug }) => (

        <ClientProfileCard clientDetails={data}, slug={slug}/>
    ))
  }
</section>
